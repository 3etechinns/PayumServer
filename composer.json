{
    "name": "payum/payum-server",
    "type": "project",
    "description": "PHP 5.5+ Payment processing server. Setup once and rule them all",
    "homepage": "http://payum.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Kotlyar Maksim",
            "email": "kotlyar.maksim@gmail.com"
        },
        {
            "name":     "Payum project",
            "homepage": "http://payum.org"
        },
        {
            "name":     "Community contributions",
            "homepage": "https://github.com/Payum/PayumServer/contributors"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-mongo": "~1.6",
        "silex/silex": "~1.2",
        "payum/payum-silex-provider": "1.0.*@dev",
        "payum/payum": "dev-payum-server-fixes as 1.0.0",
        "payum/omnipay-bridge": "1.0.*@dev",
        "symfony/yaml": "~2.8",
        "symfony/form": "~2.8",
        "symfony/validator": "~2.8",
        "symfony/config": "~2.8",
        "symfony/translation": "~2.8",
        "symfony/locale": "~2.8",
        "symfony/security-csrf": "~2.8",
        "symfony/options-resolver": "~2.8",
        "symfony/twig-bridge": "~2.8",
        "doctrine/mongodb": "~1.2",
        "jdesrosiers/silex-cors-provider": "0.1.5",
        "omnipay/omnipay": "~2.3",
        "raven/raven": "~0.10.0",
        "twig/twig": "~1.12",
        "makasim/values": "0.1.*@dev",
        "paypal/rest-api-sdk-php": "~1.0",
        "klarna/checkout": "~2.0",
        "fp/klarna-invoice": "~0.1",
        "authorizenet/authorizenet": "~1.0",
        "stripe/stripe-php": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "symfony/filesystem": "~2.8",
        "symfony/browser-kit": "~2.8",
        "symfony/css-selector": "~2.8"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:formapro-forks/Payum.git"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Payum\\Server": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}

