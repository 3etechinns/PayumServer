{
    "name": "payum/payum-server",
    "type": "project",
    "description": "PHP 5.5+ Payment processing server. Setup once and rule them all",
    "homepage": "http://payum.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Kotlyar Maksim",
            "email": "kotlyar.maksim@gmail.com"
        },
        {
            "name":     "Payum project",
            "homepage": "http://payum.org"
        },
        {
            "name":     "Community contributions",
            "homepage": "https://github.com/Payum/PayumServer/contributors"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-mongodb": "^1.1.2",
        "silex/silex": "^1",
        "payum/payum-silex-provider": "^1@beta",
        "payum/payum": "^1.3.10",
        "payum/omnipay-bridge": "^1",
        "symfony/yaml": "^3",
        "symfony/form": "3.0.*",
        "symfony/validator": "^3",
        "symfony/config": "^3",
        "symfony/translation": "^3",
        "symfony/intl": "^3",
        "symfony/security-csrf": "^3",
        "symfony/options-resolver": "^3",
        "symfony/twig-bridge": "^3",
        "php-http/guzzle6-adapter": "^1",
        "makasim/yadm": "0.1.*@dev",
        "mongodb/mongodb": "^1",
        "jdesrosiers/silex-cors-provider": "0.1.*",
        "omnipay/omnipay": "^2.3",
        "sentry/sentry": "^1",
        "twig/twig": "^1.30",
        "paypal/rest-api-sdk-php": "^1",
        "klarna/checkout": "^2",
        "fp/klarna-invoice": "^0.1",
        "authorizenet/authorizenet": "^1",
        "stripe/stripe-php": "^3",
        "sofort/sofortlib-php": "^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "symfony/filesystem": "^3",
        "symfony/browser-kit": "^3",
        "symfony/css-selector": "^3"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Payum\\Server": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}

